# Variables:
#   $devices (number) - number of devices users can connect to VPN
vpn-pricing-connect-up-to =
    { $devices ->
        [one] Zwjazajće do { $devices } grata
        [two] Zwjazajće do { $devices } gratow
        [few] Zwjazajće do { $devices } gratow
       *[other] Zwjazajće do { $devices } gratow
    }
# Variables:
#   $devices (number) - number of devices users can connect to VPN
vpn-pricing-connect-up-to-platforms =
    { $devices ->
        [one] Zwjazajće so z hač do { $devices } gratom Android, iOS, Windows, macOS abo Linux
        [two] Zwjazajće so z hač do { $devices } gratomaj Android, iOS, Windows, macOS abo Linux
        [few] Zwjazajće so z hač do { $devices } gratami Android, iOS, Windows, macOS abo Linux
       *[other] Zwjazajće so z hač do { $devices } gratami Android, iOS, Windows, macOS abo Linux
    }
# Variables:
#   $servers (number) - number of VPN servers
#   $countries (number) - number of available countries
vpn-pricing-access =
    { $servers ->
        [one] Mějće přistup k { $servers } serwerej we wjace hač { $countries } krajach
        [two] Mějće přistup k { $servers } serweromaj we wjace hač { $countries } krajach
        [few] Mějće přistup k { $servers } serweram we wjace hač { $countries } krajach
       *[other] Mějće přistup k { $servers } serweram we wjace hač { $countries } krajach
    }
