# Variables:
#   $devices (number) - number of devices users can connect to VPN
vpn-pricing-connect-up-to =
    { $devices ->
        [one] Anslut upp till { $devices } enhet
       *[other] Anslut upp till { $devices } enheter
    }
# Variables:
#   $devices (number) - number of devices users can connect to VPN
vpn-pricing-connect-up-to-platforms =
    { $devices ->
        [one] Anslut upp till { $devices } Android-, iOS-, Windows, macOS eller Linux-enhet
       *[other] Anslut upp till { $devices } Android-, iOS-, Windows, macOS eller Linux-enheter
    }
# Variables:
#   $servers (number) - number of VPN servers
#   $countries (number) - number of available countries
vpn-pricing-access =
    { $servers ->
        [one] Få åtkomst till { $servers } server i över { $countries } länder
       *[other] Få åtkomst till { $servers } servrar i över { $countries } länder
    }
